{
  "name": "filecoin-pickaxe-agent",
  "version": "0.0.1",
  "description": "Agent to perform requested Filecoin tasks",
  "scripts": {
    "dev": "nodemon --ignore dist --exec babel-node index.js",
    "build": "babel *.js --out-dir dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "pickaxe-agent": "dist/index.js"
  },
  "keywords": [],
  "author": "Jim Pick (@jimpick)",
  "license": "MIT",
  "dependencies": {
    "@jimpick/filecoin-pickaxe-mineshaft": "^0.0.7",
    "bignumber.js": "^8.1.1",
    "filecoin-api-client": "^0.2.0",
    "immer": "^3.1.2",
    "peer-base": "peer-base/peer-base#fix/load-ormap-causal-context"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-shebang": "^1.0.0",
    "nodemon": "^1.18.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "shebang"
    ],
    "sourceMaps": "inline"
  },
  "browserslist": "node 10",
  "nodemonConfig": {
    "stdin": false,
    "restartable": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimpick/filecoin-pickaxe.git"
  }
}
